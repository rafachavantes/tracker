var el = { "email": {{jsc - input - email}},    // vari√°veis a serem enviadas
           "nome": {{jsc - input - nome}},
           "cidade": {{cookie - ment_ct}},
           "estado": {{cookie - ment_st}},
           "pais": {{cookie - ment_cn}},
           "utm_source": {{URL - utm_source}},
           "utm_medium": {{URL- utm_medium}},
           "utm_campaign": {{URL - utm_campaign}},
           "utm_term": {{URL - utm_term}},
